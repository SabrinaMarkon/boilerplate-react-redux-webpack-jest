{
  "name": "boilerplate-react-redux-webpack-jest",
  "version": "1.0.0",
  "description": "This is boilerplate to use with React 16 projects that includes Redux, React-Router, React Hooks, Enzyme, Jest, Snapshots, Webpack, Babel 7, SASS",
  "main": "index.js",
  "scripts": {
    "test": "jest --colors",
    "test:watch": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "start": "webpack-dev-server --mode development",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "scss": "node-sass --watch scss -o css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SabrinaMarkon/boilerplate-react-redux-webpack-jest.git"
  },
  "keywords": [
    "react",
    "redux",
    "babel",
    "jest",
    "chingu",
    "webpack"
  ],
  "author": "Sabrina Markon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SabrinaMarkon/boilerplate-react-redux-webpack-jest/issues"
  },
  "homepage": "https://github.com/SabrinaMarkon/boilerplate-react-redux-webpack-jest#readme",
  "dependencies": {
    "html-webpack-plugin": "^5.5.0",
    "node-forge": "^0.10.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "tar": "^4.4.18"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.3.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.3",
    "node-sass": "^4.14.1",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^13.2.0",
    "serialize-javascript": "^3.1.0",
    "set-value": "^4.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  }
